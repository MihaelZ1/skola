int value = 0;

void setup() {
  size(700, 700);
}

void draw() {
  background(255);
  fill(value);
  rect(100, 100, 200, 200);
}

void mousePressed() {
  if (mouseX > 100 && mouseX < 300 && mouseY > 100 && mouseY < 300) {
    if (value == 0) {
      value = 255;
    } else {
      value = 0;
    }
  }
}
