import java.util.Random;

public class Main {
    public static void main(String[] args) {
    Random ran = new Random();

    int  a = ran.nextInt(8);
    int b = ran.nextInt(8);

    int zbr=0;
    zbr=a+b;

        System.out.println("Broj: "+a);
        System.out.println("2. Broj: "+b);
        System.out.println("Zbroj je: "+zbr);

    if(zbr<10){
        System.out.println("ZBroj je manji od 10");
    }
    else
        System.out.println("ZBroj nije manji od 10");




    }



    }


import java.util.Random;

public class Main {
    public static void main(String[] args) {
    Random ran = new Random();

    int  a = ran.nextInt(4);
    //int b = ran.nextInt(8);


       if(a==0)
           System.out.println("Gnerirani slucajni broj je 0");

       else if(a==1)
           System.out.println("Gnerirani slucajni broj je 1");
       else if(a==2)
           System.out.println("Gnerirani slucajni broj je 2");
       else if(a==3)
           System.out.println("Gnerirani slucajni broj je 3");
       else if(a==4)
           System.out.println("Gnerirani slucajni broj je 4");



    }



    }

import java.util.Random;

public class Main {
    public static void main(String[] args) {
    Random ran = new Random();

    int  a = ran.nextInt(10);
    int b = ran.nextInt(10);
    int zbr1=0;
    int zbr2=0;

    System.out.println("1. broj je: "+a);
    System.out.println("2. broj je: "+b);


    zbr1=a+b;
    if(zbr1<10){
        int c= ran.nextInt(10);
        System.out.println("3. broj je: "+c);
        zbr2=zbr1+c;
        System.out.println("Zbroj sva tri broja je "+zbr2);


    }
    else
        System.out.println("Zbroj oba broja je "+zbr1);






    }



    }



import java.util.Random;

public class Main {
    public static void main(String[] args) {
    Random ran = new Random();

    int  a = ran.nextInt(10);
    int b = ran.nextInt(10);
    int kvadrat = 0;
    int suma = 0;
    int razlika = 0;



    System.out.println("1. broj je: "+a);
    System.out.println("2. broj je: "+b);


    if(a==b){
        kvadrat=a*b;
        System.out.println("Kvadrat je :"+kvadrat);
    }
    else if(a>b){
        razlika=a-b;
        System.out.println("Razlika je: "+razlika);
    }
    else {

        suma = a + b;
        0
        System.out.println("Zbroj oba broja je " + suma);
    }





    }



    }


import java.util.Random;

public class Main {
    public static void main(String[] args) {
    Random ran = new Random();

    int  a = ran.nextInt(25);
    //int b = ran.nextInt(10);
    
    System.out.println("1. broj je: "+a);
    //System.out.println("2. broj je: "+b);


   if(a>10&&a<20){
       System.out.println("Broj je između 10 i 20");
   }
else{
    if(a%2==0)
        System.out.println("Broj je paran");
    else
        System.out.println("Broj je neparan");
    if(a>0)
        System.out.println("Broj je pozitivan");
    else
        System.out.println("Broj je negativan");

    }

   }


   }






import java.util.Random;

public class Main {
    public static void main(String[] args) {
    Random ran = new Random();

    int  a = ran.nextInt(15);
    //int b = ran.nextInt(10);

    System.out.println("1. broj je: "+a);
    //System.out.println("2. broj je: "+b);


   if(a>10&&a%2==0){
       System.out.println("Broj je veci od 10");
   }
   else 
       System.out.println("Broj je manji od 10");
    
   if(a*a==25)
    System.out.println("Kvadrat mu je jednak 25");
   
    else 
        System.out.println("Kvadrat mu nije jednak 25");
    
    
    }


   }




import java.util.Random;

public class Main {
    public static void main(String[] args) {
    Random ran = new Random();

    int  a = ran.nextInt(10)-5;
    int b = ran.nextInt(10)-5;

    System.out.println("1. broj je: "+a);
    System.out.println("2. broj je: "+b);


   if(a<0&&b<0)
       System.out.println("Oba broja su manja od 0");

    if(a<0||b<0)
        System.out.println("Jedan je negativan");
    }
    else
      System.out.println("Oba su pozitivna");

   }





    




