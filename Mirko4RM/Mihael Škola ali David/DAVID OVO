using System;
using System.Collections.Generic;
using System.ComponentModel;
using System.Data;
using System.Data.SqlClient;
using System.Drawing;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Windows.Forms;

namespace Dnevnik
{
    public partial class DodavanjeKnjiga : Form
    {
        private string connectionString =
            "Server=localhost\\sqlexpress;Database=Knjiznica;User ID=DavidJambrosic;Password=ManCity33;Trusted_Connection=True;";
        public DodavanjeKnjiga()
        {
            InitializeComponent();
        }

        private void DodavanjeKnjiga_Load(object sender, EventArgs e)
        {

        }
       

        private void btnDodaj_Click(object sender, EventArgs e)
        {
            int godina;
            if (!int.TryParse(txtGodina.Text, out godina))
            {
                MessageBox.Show("Godina mora biti broj.");
                return;
            }
            using (SqlConnection conn = new SqlConnection(connectionString))
            {
                string query = "INSERT INTO Knjige (Naslov, Autor, GodinaIzdavanja, ISBN, Aktivan) " +
               "VALUES (@Naslov, @Autor, @Godina, @ISBN, 1)";

                SqlCommand cmd = new SqlCommand(query, conn);
                cmd.Parameters.AddWithValue("@Naslov", txtNaslov.Text);
                cmd.Parameters.AddWithValue("@Autor", txtAutor.Text);
                cmd.Parameters.AddWithValue("@Godina", int.Parse(txtGodina.Text));
                cmd.Parameters.AddWithValue("@ISBN", txtISBN.Text);

                conn.Open();
                cmd.ExecuteNonQuery();
                conn.Close();

                MessageBox.Show("Knjiga uspješno dodana!");
                this.Close();
            }
        }
    }
}
using System;
using System.Collections.Generic;
using System.ComponentModel;
using System.Data;
using System.Data.SqlClient;
using System.Drawing;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Windows.Forms;

namespace Dnevnik
{
    public partial class UredivanjeKnjiga : Form
    {
        private string connectionString =
            "Server=localhost\\sqlexpress;Database=Knjiznica;User ID=DavidJambrosic;Password=ManCity33;Trusted_Connection=True;";
        private int knjigaID;
        public UredivanjeKnjiga()
        {
            InitializeComponent();
            LoadKnjige();
        }
        private void LoadKnjige(string filter = "")
        {
            using (SqlConnection conn = new SqlConnection(connectionString))
            {
                string query = "SELECT * FROM Knjige WHERE Aktivan = 1";

                if (!string.IsNullOrEmpty(filter))
                    query += " AND (Naslov LIKE @Filter OR Autor LIKE @Filter)"; 

                SqlDataAdapter da = new SqlDataAdapter(query, conn);

                if (!string.IsNullOrEmpty(filter))
                    da.SelectCommand.Parameters.AddWithValue("@Filter", "%" + filter + "%");

                DataTable dt = new DataTable();
                da.Fill(dt);
                dgvKnjige.DataSource = dt;

                dgvKnjige.ReadOnly = true; 
                dgvKnjige.AllowUserToAddRows = false; 
                dgvKnjige.AllowUserToDeleteRows = false; 
            }
        }
        private void dgvKnjige_CellClick(object sender, DataGridViewCellEventArgs e)
        {
            if (e.RowIndex >= 0)
            {
                DataGridViewRow row = dgvKnjige.Rows[e.RowIndex];
                knjigaID = Convert.ToInt32(row.Cells["KnjigaID"].Value);
                txtNaslov.Text = row.Cells["Naslov"].Value.ToString();
                txtAutor.Text = row.Cells["Autor"].Value.ToString();
                txtGodina.Text = row.Cells["GodinaIzdavanja"].Value.ToString();
                txtISBN.Text = row.Cells["ISBN"].Value.ToString();
            }
        }


        private void UredivanjeKnjiga_Load(object sender, EventArgs e)
        {

        }

        private void btnUredi_Click(object sender, EventArgs e)
        {
            if (string.IsNullOrEmpty(txtNaslov.Text) || string.IsNullOrEmpty(txtAutor.Text) ||
                string.IsNullOrEmpty(txtGodina.Text) || string.IsNullOrEmpty(txtISBN.Text))
            {
                MessageBox.Show("Sva polja moraju biti popunjena!");
                return;
            }

            using (SqlConnection conn = new SqlConnection(connectionString))
            {
                string query = "UPDATE Knjige SET Naslov=@Naslov, Autor=@Autor, GodinaIzdavanja=@Godina, ISBN=@ISBN WHERE KnjigaID=@ID";
                SqlCommand cmd = new SqlCommand(query, conn);
                cmd.Parameters.AddWithValue("@Naslov", txtNaslov.Text);
                cmd.Parameters.AddWithValue("@Autor", txtAutor.Text);
                cmd.Parameters.AddWithValue("@Godina", int.Parse(txtGodina.Text));
                cmd.Parameters.AddWithValue("@ISBN", txtISBN.Text);
                cmd.Parameters.AddWithValue("@ID", knjigaID);

                conn.Open();
                cmd.ExecuteNonQuery();
                conn.Close();

                MessageBox.Show("Knjiga uspješno ažurirana!");
                LoadKnjige();
            }
        }

        private void btnPretrazi_Click(object sender, EventArgs e)
        {
            LoadKnjige(txtPretraga.Text);
        }
    }
}
