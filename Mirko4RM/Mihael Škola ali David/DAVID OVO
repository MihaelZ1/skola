        private string connectionString =
            "Server=localhost\\sqlexpress;Database=Knjiznica;User ID=DavidJambrosic;Password=ManCity33;Trusted_Connection=True;";
        private int clanID;
        public UredivanjeClanova()
        {
            InitializeComponent();
            LoadClanovi();
        }

        private void UredivanjeClanova_Load(object sender, EventArgs e)
        {

        }
        private void LoadClanovi(string filter = "")
        {
            using (SqlConnection conn = new SqlConnection(connectionString))
            {
                string query = "SELECT * FROM Clanovi WHERE Aktivan = 1";

                if (!string.IsNullOrEmpty(filter))
                    query += " AND (Ime LIKE @Filter OR Prezime LIKE @Filter)";

                SqlDataAdapter da = new SqlDataAdapter(query, conn);

                if (!string.IsNullOrEmpty(filter))
                    da.SelectCommand.Parameters.AddWithValue("@Filter", "%" + filter + "%");

                DataTable dt = new DataTable();
                da.Fill(dt);
                dgvClanovi.DataSource = dt;
                dgvClanovi.Columns["ClanID"].Visible = false;
            }
        }

        private void btnPretrazi_Click(object sender, EventArgs e)
        {
            LoadClanovi(txtPretraga.Text);
        }
        private void dgvClanovi_CellClick(object sender, DataGridViewCellEventArgs e)
        {
            if (e.RowIndex >= 0)
            {
                clanID = Convert.ToInt32(dgvClanovi.Rows[e.RowIndex].Cells["ClanID"].Value);
                txtIme.Text = dgvClanovi.Rows[e.RowIndex].Cells["Ime"].Value.ToString();
                txtPrezime.Text = dgvClanovi.Rows[e.RowIndex].Cells["Prezime"].Value.ToString();
                txtEmail.Text = dgvClanovi.Rows[e.RowIndex].Cells["Email"].Value.ToString();
                txtTelefon.Text = dgvClanovi.Rows[e.RowIndex].Cells["Telefon"].Value.ToString();
                txtDatumUclanjenja.Text = dgvClanovi.Rows[e.RowIndex].Cells["DatumUclanjenja"].Value.ToString();
            }
        }

        private void btnSpremi_Click(object sender, EventArgs e)
        {
            using (SqlConnection conn = new SqlConnection(connectionString))
            {
                string query = "UPDATE Clanovi SET Ime=@Ime, Prezime=@Prezime, Email=@Email, Telefon=@Telefon WHERE ClanID=@ID";
                SqlCommand cmd = new SqlCommand(query, conn);
                cmd.Parameters.AddWithValue("@Ime", txtIme.Text);
                cmd.Parameters.AddWithValue("@Prezime", txtPrezime.Text);
                cmd.Parameters.AddWithValue("@Email", txtEmail.Text);
                cmd.Parameters.AddWithValue("@Telefon", txtTelefon.Text);
                cmd.Parameters.AddWithValue("@ID", clanID);

                conn.Open();
                cmd.ExecuteNonQuery();
                conn.Close();

                MessageBox.Show("Član uspješno ažuriran!");
                LoadClanovi();
            }
        }
    }
}using System;
using System.Data;
using System.Data.SqlClient;
using System.Windows.Forms;

namespace Dnevnik
{
    public partial class BrisanjeClanova : Form
    {
        private string connectionString =
            "Server=localhost\\sqlexpress;Database=Knjiznica;User ID=DavidJambrosic;Password=ManCity33;Trusted_Connection=True;";
        private int clanID;
        public BrisanjeClanova()
        {
            InitializeComponent();
            LoadClanovi();
        }
        private void LoadClanovi(string filter = "")
        {
            using (SqlConnection conn = new SqlConnection(connectionString))
            {
                string query = "SELECT ClanID, Ime, Prezime, Email, Telefon, DatumUclanjenja FROM Clanovi WHERE Aktivan = 1";

                if (!string.IsNullOrEmpty(filter))
                    query += " AND (Ime LIKE @Filter OR Prezime LIKE @Filter)";

                SqlDataAdapter da = new SqlDataAdapter(query, conn);

                if (!string.IsNullOrEmpty(filter))
                    da.SelectCommand.Parameters.AddWithValue("@Filter", "%" + filter + "%");

                DataTable dt = new DataTable();
                da.Fill(dt);
                dgvClanovi.DataSource = dt;
                dgvClanovi.Columns["ClanID"].Visible = false; 
            }
        }
        private void dgvClanovi_CellClick(object sender, DataGridViewCellEventArgs e)
        {
            if (e.RowIndex >= 0)
            {
                DataGridViewRow row = dgvClanovi.Rows[e.RowIndex];
                clanID = Convert.ToInt32(row.Cells["ClanID"].Value);
                txtIme.Text = row.Cells["Ime"].Value.ToString();
                txtPrezime.Text = row.Cells["Prezime"].Value.ToString();
                txtEmail.Text = row.Cells["Email"].Value.ToString();
                txtTelefon.Text = row.Cells["Telefon"].Value.ToString();
                txtDatumUclanjenja.Text = row.Cells["DatumUclanjenja"].Value.ToString();
            }
        }
        private void BrisanjeClanova_Load(object sender, EventArgs e)
        {
            LoadClanovi();
        }

        private void btnPretrazi_Click(object sender, EventArgs e)
        {
            LoadClanovi(txtPretraga.Text);
        }

        private void btnIzbrisi_Click(object sender, EventArgs e)
        {
            if (clanID > 0)
            {
                var confirm = MessageBox.Show("Jeste li sigurni da želite označiti člana kao neaktivnog?",
                                              "Potvrda brisanja", MessageBoxButtons.YesNo);
                if (confirm == DialogResult.Yes)
                {
                    using (SqlConnection conn = new SqlConnection(connectionString))
                    {
                        string query = "UPDATE Clanovi SET Aktivan = 0 WHERE ClanID=@ID";
                        SqlCommand cmd = new SqlCommand(query, conn);
                        cmd.Parameters.AddWithValue("@ID", clanID);

                        conn.Open();
                        cmd.ExecuteNonQuery();
                        conn.Close();

                        MessageBox.Show("Član je označen kao neaktivan!");
                        LoadClanovi(); 
                    }
                }
            }
            else
            {
                MessageBox.Show("Morate odabrati člana iz tablice prije nego što ga označite kao neaktivnog.");
            }
        }
    }
}
using System;
using System.Collections.Generic;
using System.ComponentModel;
using System;
using System.Data.SqlClient;
using System.Windows.Forms;
