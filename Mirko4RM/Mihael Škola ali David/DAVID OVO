using System;
using System.Data;
using System.Data.SqlClient;
using System.Windows.Forms;

namespace Dnevnik
{
    public partial class PosudbaKnjiga : Form
    {
        private string connectionString =
            "Server=localhost\\sqlexpress;Database=Knjiznica;User ID=DavidJambrosic;Password=ManCity33;Trusted_Connection=True;";
        private int clanID;
        private int knjigaID;
        public PosudbaKnjiga()
        {
            InitializeComponent();
            LoadClanovi();
            LoadKnjige();
        }

        private void PosudbaKnjiga_Load(object sender, EventArgs e)
        {

        }
        private void LoadClanovi(string filter = "")
        {
            using (SqlConnection conn = new SqlConnection(connectionString))
            {
                string query = "SELECT ClanID, Ime, Prezime, Email FROM Clanovi WHERE Aktivan = 1";
                if (!string.IsNullOrEmpty(filter))
                    query += " AND (Ime LIKE @Filter OR Prezime LIKE @Filter)";

                SqlDataAdapter da = new SqlDataAdapter(query, conn);
                if (!string.IsNullOrEmpty(filter))
                    da.SelectCommand.Parameters.AddWithValue("@Filter", "%" + filter + "%");

                DataTable dt = new DataTable();
                da.Fill(dt);
                dgvClanovi.DataSource = dt;
            }
        }
        private void LoadKnjige(string filter = "")
        {
            using (SqlConnection conn = new SqlConnection(connectionString))
            {
                string query = "SELECT KnjigaID, Naslov, Autor, Status FROM Knjige WHERE Aktivan = 1 AND Status='Dostupna'";
                if (!string.IsNullOrEmpty(filter))
                    query += " AND Naslov LIKE @Filter";

                SqlDataAdapter da = new SqlDataAdapter(query, conn);
                if (!string.IsNullOrEmpty(filter))
                    da.SelectCommand.Parameters.AddWithValue("@Filter", "%" + filter + "%");

                DataTable dt = new DataTable();
                da.Fill(dt);
                dgvKnjige.DataSource = dt;
            }
        }

        private void btnPosudi_Click(object sender, EventArgs e)
        {
            if (clanID <= 0 || knjigaID <= 0)
            {
                MessageBox.Show("Morate odabrati člana i knjigu prije posudbe.", "Greška", MessageBoxButtons.OK, MessageBoxIcon.Error);
                return; 
            }

            try
            {
                using (SqlConnection conn = new SqlConnection(connectionString))
                {
                    string query = @"
                INSERT INTO Posudbe (ClanID, KnjigaID, DatumPosudbe, Vracena) 
                VALUES (@ClanID, @KnjigaID, GETDATE(), 0);  -- 0 označava da knjiga nije vraćena

                UPDATE Knjige SET Status='Posuđena' WHERE KnjigaID=@KnjigaID;
            ";

                    SqlCommand cmd = new SqlCommand(query, conn);
                    cmd.Parameters.AddWithValue("@ClanID", clanID);
                    cmd.Parameters.AddWithValue("@KnjigaID", knjigaID);

                    conn.Open();
                    cmd.ExecuteNonQuery();
                    conn.Close();

                    MessageBox.Show("Knjiga uspješno posuđena!", "Success", MessageBoxButtons.OK, MessageBoxIcon.Information);
                    LoadKnjige(); 
                }
            }
            catch (Exception ex)
            {
                MessageBox.Show("Došlo je do pogreške prilikom posudbe knjige: " + ex.Message, "Greška", MessageBoxButtons.OK, MessageBoxIcon.Error);
            }
        }

        private void button1_Click(object sender, EventArgs e)
        {
            LoadClanovi(txtPretragaClan.Text);
        }

        private void button2_Click(object sender, EventArgs e)
        {
            LoadKnjige(txtPretragaKnjiga.Text);
        }
        private void dgvClanovi_CellClick(object sender, DataGridViewCellEventArgs e)
        {
            if (e.RowIndex >= 0)
                clanID = Convert.ToInt32(dgvClanovi.Rows[e.RowIndex].Cells["ClanID"].Value);
        }

        private void dgvKnjige_CellClick(object sender, DataGridViewCellEventArgs e)
        {
            if (e.RowIndex >= 0)
                knjigaID = Convert.ToInt32(dgvKnjige.Rows[e.RowIndex].Cells["KnjigaID"].Value);
        }
    }
}
using System;
using System.Data;
using System.Data.SqlClient;
using System.Windows.Forms;

namespace Dnevnik
{
    public partial class PovratKnjiga : Form
    {
        private string connectionString =
            "Server=localhost\\sqlexpress;Database=Knjiznica;User ID=DavidJambrosic;Password=ManCity33;Trusted_Connection=True;";
        private int posudbaID;
        public PovratKnjiga()
        {
            InitializeComponent();
            LoadPosudbe();
        }
        private void LoadPosudbe(string filter = "")
        {
            using (SqlConnection conn = new SqlConnection(connectionString))
            {
                string query = @"SELECT p.PosudbaID, c.Ime, c.Prezime, k.Naslov, p.DatumPosudbe 
                                 FROM Posudbe p
                                 JOIN Clanovi c ON p.ClanID = c.ClanID
                                 JOIN Knjige k ON p.KnjigaID = k.KnjigaID
                                 WHERE p.Vracena = 0";

                if (!string.IsNullOrEmpty(filter))
                    query += " AND k.Naslov LIKE @Filter";

                SqlDataAdapter da = new SqlDataAdapter(query, conn);
                if (!string.IsNullOrEmpty(filter))
                    da.SelectCommand.Parameters.AddWithValue("@Filter", "%" + filter + "%");

                DataTable dt = new DataTable();
                da.Fill(dt);
                dgvPosudbe.DataSource = dt;
            }
        }
        private void dgvPosudbe_CellClick(object sender, DataGridViewCellEventArgs e)
        {
            if (e.RowIndex >= 0)
                posudbaID = Convert.ToInt32(dgvPosudbe.Rows[e.RowIndex].Cells["PosudbaID"].Value);
        }

        private void PovratKnjiga_Load(object sender, EventArgs e)
        {

        }

        private void btnPretrazi_Click(object sender, EventArgs e)
        {
            LoadPosudbe(txtPretraga.Text);
        }

        private void btnVrati_Click(object sender, EventArgs e)
        {
            if (posudbaID > 0)
            {
                using (SqlConnection conn = new SqlConnection(connectionString))
                {
                    string query = @"UPDATE Posudbe 
                                     SET DatumPovratka = GETDATE(), Vracena = 1 
                                     WHERE PosudbaID=@PosudbaID;

                                     UPDATE Knjige SET Status='Dostupna' 
                                     WHERE KnjigaID=(SELECT KnjigaID FROM Posudbe WHERE PosudbaID=@PosudbaID)";

                    SqlCommand cmd = new SqlCommand(query, conn);
                    cmd.Parameters.AddWithValue("@PosudbaID", posudbaID);

                    conn.Open();
                    cmd.ExecuteNonQuery();
                    conn.Close();

                    MessageBox.Show("Knjiga uspješno vraćena!");
                    LoadPosudbe(); 
                }
            }
        }
    }
}
using System;
using System.Collections.Generic;
using System.ComponentModel;
using System.Data;
using System.Data.SqlClient;
using System.Drawing;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Windows.Forms;

namespace Dnevnik
{
    public partial class Statistika : Form
    {
        private string connectionString =
            "Server=localhost\\sqlexpress;Database=Knjiznica;User ID=DavidJambrosic;Password=ManCity33;Trusted_Connection=True;";
        public Statistika()
        {
            InitializeComponent();
            LoadStatistika();
        }

        private void Statistika_Load(object sender, EventArgs e)
        {

        }
        private void LoadStatistika()
        {
            using (SqlConnection conn = new SqlConnection(connectionString))
            {
                conn.Open();

                SqlCommand cmdKnjige = new SqlCommand("SELECT COUNT(*) FROM Knjige WHERE Aktivan = 1", conn);
                int brojKnjiga = (int)cmdKnjige.ExecuteScalar();
                lblBrojKnjiga.Text = $"Aktivne knjige: {brojKnjiga}";

                SqlCommand cmdClanovi = new SqlCommand("SELECT COUNT(*) FROM Clanovi WHERE Aktivan = 1", conn);
                int brojClanova = (int)cmdClanovi.ExecuteScalar();
                lblBrojClanova.Text = $"Aktivni članovi: {brojClanova}";

                SqlCommand cmdPosudjene = new SqlCommand("SELECT COUNT(*) FROM Knjige WHERE Status = 'Posuđena' AND Aktivan = 1", conn);
                int brojPosudjenih = (int)cmdPosudjene.ExecuteScalar();
                lblPosudjene.Text = $"Posuđene knjige: {brojPosudjenih}";

                SqlCommand cmdUkupnoPosudbi = new SqlCommand("SELECT COUNT(*) FROM Posudbe", conn);
                int ukupnoPosudbi = (int)cmdUkupnoPosudbi.ExecuteScalar();
                lblUkupnoPosudbi.Text = $"Ukupno posudbi: {ukupnoPosudbi}";

                SqlCommand cmdPosudbePoKnjigama = new SqlCommand(@"SELECT k.Naslov, COUNT(p.PosudbaID) AS Posudbe
                                                                FROM Posudbe p
                                                                JOIN Knjige k ON p.KnjigaID = k.KnjigaID
                                                                WHERE k.Aktivan = 1
                                                                GROUP BY k.Naslov", conn);
                SqlDataReader reader = cmdPosudbePoKnjigama.ExecuteReader();
                StringBuilder sb = new StringBuilder();
                while (reader.Read())
                {
                    sb.AppendLine($"{reader["Naslov"]}: {reader["Posudbe"]} posudbi");
                }
                reader.Close();
                lblPosudbePoKnjigama.Text = sb.ToString();

                SqlCommand cmdNeaktivniClanovi = new SqlCommand("SELECT COUNT(*) FROM Clanovi WHERE Aktivan = 0", conn);
                int brojNeaktivnihClanova = (int)cmdNeaktivniClanovi.ExecuteScalar();
                lblNeaktivniClanovi.Text = $"Neaktivni članovi: {brojNeaktivnihClanova}";

                SqlCommand cmdProsjecnoTrajanje = new SqlCommand(@"SELECT AVG(DATEDIFF(DAY, p.DatumPosudbe, p.DatumPovratka)) 
                                                                FROM Posudbe p
                                                                WHERE p.DatumPovratka IS NOT NULL", conn);
                object result = cmdProsjecnoTrajanje.ExecuteScalar();
                if (result != DBNull.Value)
                {
                    int prosječnoTrajanje = (int)result;
                    lblProsjecnoTrajanje.Text = $"Prosječno trajanje posudbe: {prosječnoTrajanje} dana";
                }

                SqlCommand cmdNevraceneKnjige = new SqlCommand("SELECT COUNT(*) FROM Posudbe WHERE Vracena = 0", conn);
                int brojNevracenih = (int)cmdNevraceneKnjige.ExecuteScalar();
                lblNevraceneKnjige.Text = $"Nevraćene knjige: {brojNevracenih}";

                conn.Close();
            }
        }
    }
}
