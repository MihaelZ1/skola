namespace Dnevnik
{
    public partial class PregledClanova : Form
    {
        private string connectionString =
            "Server=localhost\\sqlexpress;Database=Knjiznica;User ID=DavidJambrosic;Password=ManCity33;Trusted_Connection=True;";
        public PregledClanova()
        {
            InitializeComponent();
            LoadClanovi();
        }
        private void LoadClanovi(string filter = "")
        {
            using (SqlConnection conn = new SqlConnection(connectionString))
            {
                string query = "SELECT ClanID, Ime, Prezime, Email, Telefon, DatumUclanjenja FROM Clanovi WHERE Aktivan = 1";

                if (!string.IsNullOrEmpty(filter))
                    query += " AND (Ime LIKE @Filter OR Prezime LIKE @Filter)";

                SqlDataAdapter da = new SqlDataAdapter(query, conn);

                if (!string.IsNullOrEmpty(filter))
                    da.SelectCommand.Parameters.AddWithValue("@Filter", "%" + filter + "%");

                DataTable dt = new DataTable();
                da.Fill(dt);
                dgvClanovi.DataSource = dt;

                dgvClanovi.Columns["ClanID"].Visible = false;
            }
        }

        private void PregledClanova_Load(object sender, EventArgs e)
        {

        }

        private void button1_Click(object sender, EventArgs e)
        {
            LoadClanovi(txtPretraga.Text);
        }
    }
