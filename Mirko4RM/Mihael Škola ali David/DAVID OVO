namespace Dnevnik
{
    public partial class DodavanjeClanova : Form
    {
        private string connectionString =
            "Server=localhost\\sqlexpress;Database=Knjiznica;User ID=DavidJambrosic;Password=ManCity33;Trusted_Connection=True;";
        public DodavanjeClanova()
        {
            InitializeComponent();
        }

        private void DodavanjeClanova_Load(object sender, EventArgs e)
        {

        }

        private void btnDodaj_Click(object sender, EventArgs e)
        {
            using (SqlConnection conn = new SqlConnection(connectionString))
            {
                string query = @"INSERT INTO Clanovi (Ime, Prezime, Email, Telefon, DatumUclanjenja, Aktivan) 
                                 VALUES (@Ime, @Prezime, @Email, @Telefon, GETDATE(), 1)";

                SqlCommand cmd = new SqlCommand(query, conn);
                cmd.Parameters.AddWithValue("@Ime", txtIme.Text);
                cmd.Parameters.AddWithValue("@Prezime", txtPrezime.Text);
                cmd.Parameters.AddWithValue("@Email", txtEmail.Text);
                cmd.Parameters.AddWithValue("@Telefon", txtTelefon.Text);

                conn.Open();
                cmd.ExecuteNonQuery();
                conn.Close();

                MessageBox.Show("Član uspješno dodan!");

                // očisti polja nakon dodavanja
                txtIme.Clear();
                txtPrezime.Clear();
                txtEmail.Clear();
                txtTelefon.Clear();
            }
        }
    }
}
using System;
using System.Collections.Generic;
using System.ComponentModel;
using System.Data;
using System.Data.SqlClient;
using System.Drawing;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
